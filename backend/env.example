# MongoDB Atlas 설정 (실제 값으로 변경 필요)
MONGODB_URL=mongodb+srv://<username>:<password>@<cluster-name>.mongodb.net/<database-name>?retryWrites=true&w=majority
MONGODB_DATABASE=ketohelper

# JWT 설정
SECRET_KEY=your-super-secret-key-change-this-in-production-ketohelper-2024
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# 애플리케이션 설정
DEBUG=true
PROJECT_NAME=KetoHelper
VERSION=1.0.0

# 서버 설정
HOST=0.0.0.0
PORT=8000

# CORS 설정
CORS_ORIGINS=http://localhost:3000,http://localhost:5173,http://127.0.0.1:3000

# Google OAuth 설정 (나중에 추가)
# GOOGLE_CLIENT_ID=your-google-client-id
# GOOGLE_CLIENT_SECRET=your-google-client-secret

# OpenAI API 설정 (나중에 추가)
# OPENAI_API_KEY=your-openai-api-key

# 로그 레벨
LOG_LEVEL=INFO
