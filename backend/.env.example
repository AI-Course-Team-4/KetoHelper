# 데이터베이스 설정
SUPABASE_URL=insert-your-key
SUPABASE_ANON_KEY=insert-your-key
SUPABASE_SERVICE_ROLE_KEY=insert-your-key

# AI/LLM 설정 (OpenAI API 키 필요)
OPENAI_API_KEY=insert-your-key
EMBEDDING_MODEL=text-embedding-3-small
#LLM_MODEL=gpt-3.5-turbo
LLM_MODEL = gemini-1.5-flash
# Google Gemini API 키(OpenAI API 키 필요)
GOOGLE_API_KEY=insert-your-key
# Gemini 세부 설정
# GEMINI_TEMPERATURE: AI 응답의 창의성 조절 (0.0=일관성, 1.0=창의성)
# GEMINI_MAX_TOKENS: 최대 응답 길이 (토큰 수)
GEMINI_TEMPERATURE=0.1
GEMINI_MAX_TOKENS=8192

# 카카오 API 설정 (카카오 디벨로퍼스에서 발급)
KAKAO_REST_KEY=insert-your-key

# 애플리케이션 설정
DEBUG=true
ENVIRONMENT=development

# RAG 설정
MAX_SEARCH_RESULTS=5
SIMILARITY_THRESHOLD=0.7

# 캐시 설정
ENABLE_CACHE=true
CACHE_TTL_SECONDS=3600

# Naver OAuth 설정
VITE_NAVER_CLIENT_ID=insert-your-key
NAVER_CLIENT_SECRET=insert-your-key