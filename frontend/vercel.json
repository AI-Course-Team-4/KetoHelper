{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cross-Origin-Opener-Policy", "value": "same-origin-allow-popups" }
      ]
    }
  ],
  "routes": [
    { "src": "/api/v1/(.*)", "dest": "https://ketohelper-production.up.railway.app/api/v1/$1" },
    { "src": "/((?!.*\\..*).*)", "dest": "/index.html" }
  ]
}
